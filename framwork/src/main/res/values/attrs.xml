<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProgressButtonStyle">
        <attr name="pb_radius" format="dimension|float" />
        <attr name="pb_btn_bg" format="reference" />
        <attr name="pb_progress_color" format="reference" />
        <attr name="pb_text" format="string" />
        <attr name="pb_text_color" format="color" />
    </declare-styleable>
    <declare-styleable name="GuildPageStyle">
        <attr name="gp_btn_text" format="string" />
        <attr name="gp_btn_text_bg" format="reference" />
        <attr name="gp_btn_text_color" format="reference" />
        <attr name="gp_btn_text_size" format="dimension" />
    </declare-styleable>
    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="indicator">
            <flag name="BallPulse" value="0" />
            <flag name="BallGridPulse" value="1" />
            <flag name="BallClipRotate" value="2" />
            <flag name="BallClipRotatePulse" value="3" />
            <flag name="SquareSpin" value="4" />
            <flag name="BallClipRotateMultiple" value="5" />
            <flag name="BallPulseRise" value="6" />
            <flag name="BallRotate" value="7" />
            <flag name="CubeTransition" value="8" />
            <flag name="BallZigZag" value="9" />
            <flag name="BallZigZagDeflect" value="10" />
            <flag name="BallTrianglePath" value="11" />
            <flag name="BallScale" value="12" />
            <flag name="LineScale" value="13" />
            <flag name="LineScaleParty" value="14" />
            <flag name="BallScaleMultiple" value="15" />
            <flag name="BallPulseSync" value="16" />
            <flag name="BallBeat" value="17" />
            <flag name="LineScalePulseOut" value="18" />
            <flag name="LineScalePulseOutRapid" value="19" />
            <flag name="BallScaleRipple" value="20" />
            <flag name="BallScaleRippleMultiple" value="21" />
            <flag name="BallSpinFadeLoader" value="22" />
            <flag name="LineSpinFadeLoader" value="23" />
            <flag name="TriangleSkewSpin" value="24" />
            <flag name="Pacman" value="25" />
            <flag name="BallGridBeat" value="26" />
            <flag name="SemiCircleSpin" value="27" />
        </attr>
        <attr name="indicator_color" format="color" />
    </declare-styleable>
    <!-- end -->
    <!--swipeMenu-->
    <declare-styleable name="SwipeMenu">
        <attr name="anim_duration" format="integer" />
    </declare-styleable>
    <!--end-->
    <declare-styleable name="ExpandableTextView">
        <attr name="maxCollapsedLines" format="integer" />
        <attr name="animDuration" format="integer" />
        <attr name="animAlphaStart" format="float" />
        <attr name="expandDrawable" format="reference" />
        <attr name="collapseDrawable" format="reference" />
    </declare-styleable>
    <declare-styleable name="ShapeImageViewStyle">
        <attr name="siv_border_width" format="dimension" />
        <attr name="siv_border_color" format="color" />
        <attr name="siv_radius" format="dimension" />
        <attr name="siv_pressed_alpha" format="float" />
        <attr name="siv_pressed_color" format="color" />
        <attr name="siv_shape_type" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="circle" value="1" />
        </attr>
    </declare-styleable>
    <!--以下都是laoddatalayout自定义属性-->
    <attr name="emptyText" format="reference|string" />
    <attr name="emptyImgId" format="reference" />
    <attr name="emptyImageVisible" format="boolean" />

    <attr name="errorText" format="reference|string" />
    <attr name="errorImgId" format="reference" />
    <attr name="errorImageVisible" format="boolean" />

    <attr name="noNetWorkText" format="reference|string" />
    <attr name="noNetWorkImgId" format="reference" />
    <attr name="noNetWorkImageVisible" format="boolean" />

    <attr name="allTipTextColor" format="reference|color" />
    <attr name="allTipTextSize" format="integer" />
    <attr name="allPageBackgroundColor" format="reference|color" />

    <attr name="reloadBtnText" format="reference|string" />
    <attr name="reloadBtnTextSize" format="integer" />
    <attr name="reloadBtnTextColor" format="reference|color" />
    <attr name="reloadBtnBackgroundResource" format="reference" />
    <attr name="reloadBtnVisible" format="boolean" />
    <attr name="reloadClickArea">
        <enum name="full" value="20" />
        <enum name="button" value="21" />
    </attr>

    <declare-styleable name="LoadDataLayout">
        <attr name="loadingText" format="reference|string" />
        <attr name="loadingTextSize" format="integer" />
        <attr name="loadingTextColor" format="reference|color" />
        <attr name="loadingViewLayoutId" format="reference" />

        <attr name="emptyText" />
        <attr name="emptyImgId" />
        <attr name="emptyImageVisible" />

        <attr name="errorText" />
        <attr name="errorImgId" />
        <attr name="errorImageVisible" />

        <attr name="noNetWorkText" />
        <attr name="noNetWorkImgId" />
        <attr name="noNetWorkImageVisible" />

        <attr name="allTipTextColor" />
        <attr name="allTipTextSize" />
        <attr name="allPageBackgroundColor" />

        <attr name="reloadBtnText" />
        <attr name="reloadBtnTextSize" />
        <attr name="reloadBtnTextColor" />
        <attr name="reloadBtnBackgroundResource" />
        <attr name="reloadBtnVisible" />
        <attr name="reloadClickArea" />

    </declare-styleable>

    <declare-styleable name="SimpleArcLoader">

        <attr name="arc_style" format="enum">
            <enum name="simple_arc" value="0"></enum>
            <enum name="complete_arc" value="1"></enum>
        </attr>

        <attr name="arc_margin" format="dimension" />
        <attr name="arc_thickness" format="dimension" />

        <attr name="arc_speed" format="enum">
            <enum name="slow" value="0"></enum>
            <enum name="medium" value="1"></enum>
            <enum name="fast" value="2"></enum>
        </attr>

        <attr name="arc_colors" format="reference"></attr>

    </declare-styleable>

    <declare-styleable name="Cookie">
        <!--TextColor-->
        <attr name="cookieTitleColor" format="color" />
        <attr name="cookieMessageColor" format="color" />
        <attr name="cookieActionColor" format="color" />
        <attr name="cookieBackgroundColor" format="color" />
        <!--TextSize-->
        <attr name="titleTextSize" format="dimension" />
        <attr name="messageTextSize" format="dimension" />
        <attr name="actionTextSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="MaterialProgress">
        <attr name="mp_startColor" format="color"/>
        <attr name="mp_midColor" format="color"/>
        <attr name="mp_endColor" format="color"/>
        <attr name="mp_radius" format="dimension"/>
        <attr name="mp_strokeWidth" format="dimension"/>
        <attr name="mp_speed" format="float"/>
    </declare-styleable>

</resources>